package Dominio;
//revisar bien el tema de la lista
public class Cliente {
	private String nombre_c;
	private String clave;
	private String nick;
	private int nivel;
	private int rp;
	private int cantidad_p;
	private int cantidad_e;
	private String[] lista_personajes;
	private String[] lista_skins;
	private String servidor;
	public Cliente(String nombre_c, String clave, String nick, int nivel, int rp, int cantidad_p, int cantidad_e,
			String[] lista_personajes, String[] lista_skins, String servidor) {
		super();
		this.nombre_c = nombre_c;
		this.clave = clave;
		this.nick = nick;
		this.nivel = nivel;
		this.rp = rp;
		this.cantidad_p = cantidad_p;
		this.cantidad_e = cantidad_e;
		this.lista_personajes = lista_personajes;
		this.lista_skins = lista_skins;
		this.servidor = servidor;
	}
	public String getNombre_c() {
		return nombre_c;
	}
	public void setNombre_c(String nombre_c) {
		this.nombre_c = nombre_c;
	}
	public String getClave() {
		return clave;
	}
	public void setClave(String clave) {
		this.clave = clave;
	}
	public String getNick() {
		return nick;
	}
	public void setNick(String nick) {
		this.nick = nick;
	}
	public int getNivel() {
		return nivel;
	}
	public void setNivel(int nivel) {
		this.nivel = nivel;
	}
	public int getRp() {
		return rp;
	}
	public void setRp(int rp) {
		this.rp = rp;
	}
	public int getCantidad_p() {
		return cantidad_p;
	}
	public void setCantidad_p(int cantidad_p) {
		this.cantidad_p = cantidad_p;
	}
	public int getCantidad_e() {
		return cantidad_e;
	}
	public void setCantidad_e(int cantidad_e) {
		this.cantidad_e = cantidad_e;
	}
	public String[] getLista_personajes() {
		return lista_personajes;
	}
	public void setLista_personajes(String[] lista_personajes) {
		this.lista_personajes = lista_personajes;
	}
	public String[] getLista_skins() {
		return lista_skins;
	}
	public void setLista_skins(String[] lista_skins) {
		this.lista_skins = lista_skins;
	}
	public String getServidor() {
		return servidor;
	}
	public void setServidor(String servidor) {
		this.servidor = servidor;
	}
	
	
}
